{
  "root": true,
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import"],
  "rules": {
    "func-names": 0,
    "no-use-before-define": ["error"],
    "no-param-reassign": 0,
    "strict": 0,
    "import/no-extraneous-dependencies": 2
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "ignorePatterns": ["coverage/**/*", "commitlint.config.js"]
}
